; Clojurescript compilation options - run this in a terminal:
;
; clj -A:resources -m figwheel.main -b dev -r
;
{
 :watch-dirs ["src/cljc" "src/cljs"]
 :main                 "uitui.main"
 :asset-path           "js"
 :source-map           false
 :source-map-timestamp false
 :pretty-print         false
 :output-dir           "target/js"
 :output-to            "resources/public/js/main.js"
 :optimizations        :advanced
 :parallel-build       true
}